{
    "scripts": {
        "start": "node_modules/http-server/bin/http-server",
        "bundle": "node_modules/gulp/bin/gulp.js --module=all",
        "publish-plugin": "node_modules/gulp/bin/gulp.js publish",
        "generate-docs": "node_modules/.bin/jsdoc src/ src/errors/ -d ./docs"
    },
    "jspm": {
        "jspmNodeConversion": false,
        "format": "es6",
        "directories": {
            "baseURL": "src"
        },
        "dependencies": {
            "scss": "github:mobilexag/plugin-sass@^0.1.0",
            "text": "github:systemjs/plugin-text@^0.0.3"
        },
        "devDependencies": {
            "babel": "npm:babel-core@^5.8.24",
            "babel-runtime": "npm:babel-runtime@^5.8.24",
            "core-js": "npm:core-js@^1.1.4"
        }
    },
    "devDependencies": {
        "gulp": "^3.9.0",
        "gulp-bump": "^1.0.0",
        "jsdoc": "^3.4.0",
        "rsvp": "^3.1.0",
        "systemjs-builder": "^0.14.11",
        "yargs": "^3.31.0"
    },
    "name": "tooly-tip",
